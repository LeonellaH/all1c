		ПФ.Вставить(""ВставитьГруппу"" , Ложь);
		
		// Открытие формы настройки эталонной базы
		ФормаВыбораТипаШага = ОткрытьФорму(ПутьКФормам + ""ВыборТипаШага"",
			ПФ,
			ЭтотОбъект,,,,
			ОписаниеОповещения);
			
	КонецЕсли;

	
	
